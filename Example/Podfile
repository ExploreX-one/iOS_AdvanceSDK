source 'https://github.com/CocoaPods/Specs.git'
use_frameworks!
platform :ios, '10.0'
target 'AdvanceSDK_Example' do

  pod 'AdvanceSDK', :path => '../'
  pod 'AdvanceSDK/Mercury', :path => '../'
  pod 'AdvanceSDK/CSJ', :path => '../'
  pod 'AdvanceSDK/GDT', :path => '../'
  pod 'AdvanceSDK/KS', :path => '../'
  pod 'AdvanceSDK/BD', :path => '../'
  pod 'AdvanceSDK/Bidding', :path => '../'
#  pod 'AdvanceSDK/AdvBidding', :path => '../'
  pod 'JSONModel', '1.8.0'
  pod 'Reachability', '3.2'
  pod 'SDWebImage', '5.12.1'

  pod 'Masonry'
#  pod 'Ads-CN', '4.4.0.0'

#  pod "STDebugConsole"
#  pod 'JPFPSStatus'

  target 'AdvanceSDK_Tests' do
    inherit! :search_paths
    
  end
  
  post_install do |installer|
    installer.generated_projects.each do |project|
      project.targets.each do |target|
        target.build_configurations.each do |config|
          config.build_settings['IPHONEOS_DEPLOYMENT_TARGET'] = '11.0'
        end
      end
    end
  end

end
